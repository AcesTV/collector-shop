# ============================
# CollectorShop - Environment (Example)
# Copy this file to .env and adjust values
# ============================

# PostgreSQL
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=collector
POSTGRES_PASSWORD=CHANGE_ME
POSTGRES_DB=collectorshop

# Keycloak
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=CHANGE_ME
KEYCLOAK_DB_VENDOR=postgres
KEYCLOAK_DB_ADDR=postgres
KEYCLOAK_DB_PORT=5432
KEYCLOAK_DB_DATABASE=keycloak
KEYCLOAK_DB_USER=collector
KEYCLOAK_DB_PASSWORD=CHANGE_ME
KEYCLOAK_REALM=collector
KEYCLOAK_URL=http://keycloak:8080
KEYCLOAK_FRONTEND_CLIENT_ID=collector-frontend
KEYCLOAK_BACKEND_CLIENT_ID=collector-backend
KEYCLOAK_BACKEND_CLIENT_SECRET=CHANGE_ME

# Frontend
VITE_API_BASE_URL=/api
VITE_KEYCLOAK_URL=http://localhost:8080
VITE_KEYCLOAK_REALM=collector
VITE_KEYCLOAK_CLIENT_ID=collector-frontend

# Services Common
NODE_ENV=development
DB_HOST=postgres
DB_PORT=5432
DB_USER=collector
DB_PASSWORD=CHANGE_ME
DB_NAME=collectorshop
